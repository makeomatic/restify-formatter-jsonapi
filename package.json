{
  "name": "restify-formatter-jsonapi",
  "version": "0.1.3",
  "description": "Restify jsonapi formatter",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "babel ./src -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators",
    "pretest": "npm rebuild",
    "test": "BLUEBIRD_DEBUG=1 ./node_modules/.bin/mocha --require ./test/babelhook.js --bail -R xunit > test-report-$(node -v).xml",
    "test-local": "NODE_ENV=development ./node_modules/.bin/mocha --require ./test/babelhook.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/restify-formatter-jsonapi.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/restify-formatter-jsonapi/issues"
  },
  "homepage": "https://github.com/makeomatic/restify-formatter-jsonapi#readme",
  "dependencies": {
    "json-stringify-safe": "^5.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-mocha": "^1.0.0",
    "mocha": "^2.3.3"
  }
}
